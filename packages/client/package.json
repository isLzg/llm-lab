{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "preview": "vite preview",
    "audit:licenses": "bunx license-checker --json --exclude \"MIT,ISC,Python-2.0,Apache-2.0,BSD-2-Clause,BSD-3-Clause,LGPL-3.0-or-later,CC-BY-4.0,CC0-1.0,BlueOak-1.0.0,MPL-2.0,CC-BY-3.0,0BSD,MIT-0\" > licenses.json",
    "check:circular": "bunx madge --circular --extensions ts,tsx src"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@elysiajs/eden": "^1.4.6",
    "@google/genai": "^1.34.0",
    "@tailwindcss/vite": "^4.1.18",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.11.0",
    "tailwindcss": "^4.1.18",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  }
}
